// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  username      String   @id
  password      String
  nama_depan    String
  nama_belakang String
  alamat        String
  no_hp         String
  tanggal_lahir DateTime
  peran         String
}


